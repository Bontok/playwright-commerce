{
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn",
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
    }
  }
  